[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[alias]
	s = status
	d = diff
	ds = diff --staged
	c = commit
	b = branch
	co = checkout
	ff = merge --ff-only
	fa = fetch --all
[branch]
	autosetuprebase = always

[mergetool.ediff]
    cmd = /Applications/Emacs.app/Contents/MacOS/Emacs --eval \" \
(progn \
  (setq ediff-quit-hook 'kill-emacs) \
  (if (file-readable-p \\\"$BASE\\\") \
      (ediff-merge-files-with-ancestor \\\"$LOCAL\\\" \\\"$REMOTE\\\" \
                                       \\\"$BASE\\\" nil \\\"$MERGED\\\") \
      (edifg-merge-files \\\"$LOCAL\\\" \\\"$REMOTE\\\" nil \\\"$MERGED\\\")))\"

[merge]
       tool = ediff
[core]
	quotepath = false
[push]
	default = upstream
[mergetool]
	keepBackup = true
